from random import random
def pi(n):
    cords_x = []
    cords_y = []
    count_in = 0
    for i in range(1, n):
        x = random()
        #print(x)
        y = random()
        #print(y)
        if (x**2 + y**2) <= 1:
            count_in +=1
            #print("Less than 1")
        print(count_in/n * 4)
